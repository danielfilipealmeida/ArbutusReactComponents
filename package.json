{
  "name": "ArbutusReactComponents",
  "version": "0.0.1",
  "description": "A bunch of React Components.",
  "main": "gulpfile.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start --single-run",
    "build": "./node_modules/.bin/webpack --progress --stats --config ./webpack.config.js",
    "dev": "node --harmony ./webpack/dev-server.js",
    "server": "./node_modules/.bin/webpack-dev-server ./build/bundle.js",
    "prod": "NODE_ENV=production node server/index.js",
    "postinstall": "npm run build"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "Daniel Almeida",
  "license": "MIT",
  "dependencies": {
    "babel": "~6.5.2",
    "babel-core": "~6.14.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-react": "~6.11.1",
    "babelify": "~7.3.0",
    "dev-server": "0.0.2",
    "file-loader": "~0.9.0",
    "path": "~0.12.7",
    "react": "~15.3.1",
    "react-dom": "~15.3.1",
    "webpack": "~1.13.2"
  },
  "devDependencies": {
    "babel-loader": "~6.2.5",
    "webpack-dev-server": "~1.15.1"
  }
}
